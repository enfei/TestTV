# platform :ios, '9.0'

def shared_pods
  pod 'SwiftyJSON'
  pod 'Alamofire', '~> 4.5'
  pod 'SDWebImage', '~> 4.0'
  pod "RSLoadingView"
end



target 'testTV' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for testTV
  shared_pods
  
end

target 'testTVTests' do 
	 use_frameworks!

	shared_pods
end

